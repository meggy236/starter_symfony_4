{% extends 'public.html.twig' %}

{% block page_title_wrap %}{{ page_title(page) }}{% endblock %}

{% block head %}
    {% include 'meta.html.twig' with {
        'description' : page.content.metaDescription,
        'title' : page_title(page),
        'url' : page.path,
    } only %}
{% endblock head %}

{% block content %}
    <h1>{{ page.title }}</h1>
{% endblock content %}
